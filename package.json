{
  "name": "@openapi-typescript-infra/create",
  "version": "1.1.0",
  "description": "Project generator for @openapi-typescript-infra services",
  "type": "module",
  "exports": {
    ".": "./build/index.js",
    "./api": "./build/apiProject.js",
    "./handlers": "./build/handlers.js"
  },
  "author": "Max Metral <developers@pyralis.com>",
  "license": "MIT",
  "config": {
    "coconfig": "./coconfig.cjs"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "echo \"TODO: write tests\" && exit 0",
    "clean": "yarn dlx rimraf ./build",
    "lint": "eslint src",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "postinstall": "husky install && coconfig"
  },
  "bin": "./build/index.js",
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "yarn eslint src --cache --fix"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@openapi-typescript-infra/coconfig": "^3.2.0",
    "@types/configstore": "^6.0.0",
    "@types/handlebars-helpers": "^0.5.3",
    "@types/minimist": "^1.2.2",
    "@types/node": "^20.4.2",
    "@types/parse-git-config": "^3.0.1",
    "@types/pascalcase": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "coconfig": "^0.12.2",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "pinst": "^3.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6"
  },
  "packageManager": "yarn@3.5.1",
  "dependencies": {
    "@readme/openapi-parser": "^2.5.0",
    "boxen": "^7.1.1",
    "configstore": "^6.0.0",
    "handlebars-helpers": "^0.10.0",
    "minimist": "^1.2.8",
    "mkdirp": "^3.0.1",
    "openapi-types": "^12.1.3",
    "parse-git-config": "^3.0.0",
    "plop": "^3.1.2",
    "plop-pack-git-init": "^0.3.1"
  }
}
